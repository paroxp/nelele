{
  "name": "nevandela.io",
  "version": "0.0.1",
  "description": "Barbara Prochownik's homepage.",
  "main": "index.js",
  "scripts": {
    "build:clean": "rm -rf dist/",
    "build:prepare": "mkdir dist/",
    "build:static": "ts-node src/generator.ts",
    "build": "npm run -s build:clean && npm run -s build:prepare && npm run -s build:static",
    "fix:ts": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "fix:vulnerabilities": "npm audit fix --update-binary",
    "fix": "npm run -s fix:sass && npm run -s fix:ts",
    "lint:sass": "",
    "lint:ts": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint": "npm run -s lint:sass && npm run -s lint:ts",
    "start": "http-server ./dist",
    "test:audit": "npm audit",
    "test": "npm run -s lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paroxp/nelele.git"
  },
  "author": "Rafal Proszowski <dev@rafalp.com> (https://www.rafalp.com/)",
  "license": "MIT",
  "homepage": "https://nevandela.io/",
  "devDependencies": {
    "@types/html-minifier": "^4.0.5",
    "@types/moment": "^2.11.29",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.27.0",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import": "^2.31.0",
    "html-minifier": "^4.0.0",
    "http-server": "^14.1.1",
    "moment": "^2.30.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.93.2",
    "sitemap": "^8.0.0",
    "susy": "^3.0.8",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  }
}
